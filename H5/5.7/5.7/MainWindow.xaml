<Window x:Class="_5._7.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_5._7"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas x:Name="cvsDrawStreet" Margin="0,93,0,0"/>
        <TextBox x:Name="txbPositionX" HorizontalAlignment="Left" Margin="153,39,0,0" TextWrapping="Wrap" Text="10" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txbPositionY" HorizontalAlignment="Left" Margin="328,39,0,0" TextWrapping="Wrap" Text="10" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txbSize" HorizontalAlignment="Left" Margin="507,39,0,0" TextWrapping="Wrap" Text="20" VerticalAlignment="Top" Width="120"/>
        <Label Content="positionX" HorizontalAlignment="Left" Margin="183,13,0,0" VerticalAlignment="Top"/>
        <Label Content="positionY" HorizontalAlignment="Left" Margin="358,13,0,0" VerticalAlignment="Top"/>
        <Label Content="size" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="537,13,0,0"/>
        <Button x:Name="btnDrawStreet" Content="Draw Street" HorizontalAlignment="Left" Margin="355,62,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
